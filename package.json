{
  "name": "guess-the-movie",
  "version": "1.0.0",
  "description": "Movie guessing game",
  "main": "server.js",
  "scripts": {
    "dev:server": "nodemon server.js",
    "dev:client": "vite",
    "dev:scheduler": "nodemon scheduler.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npm run dev:scheduler\"",
    "build": "vite build",
    "serve": "vite preview",
    "start": "concurrently \"node server.js\" \"node scheduler.js\""
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.4.7",
    "dropbox": "^10.34.0",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.9",
    "vite": "^6.2.1",
    "vite-express": "^0.20.0"
  },
  "devDependencies": {
    "ngrok": "^5.0.0-beta.2"
  }
}
